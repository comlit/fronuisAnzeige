<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EDEDED">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">



        <RelativeLayout
            android:layout_height="match_parent"
            android:layout_width="378dp"
            android:orientation="vertical"
            android:backgroundTint="#B30E0E">

            <RelativeLayout
                android:id="@+id/rellay"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:backgroundTint="#833737"
                android:paddingLeft="5dp"
                android:paddingTop="5dp">

                <GridLayout
                    android:id="@+id/gridlay"
                    android:layout_width="368dp"
                    android:layout_height="242dp"
                    android:background="@color/black"
                    android:columnCount="2"
                    android:rowCount="2">

                    <RelativeLayout
                        android:layout_width="184dp"
                        android:layout_height="121dp"
                        android:background="@color/gray_600">

                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="174dp"
                            android:layout_height="111dp"
                            android:layout_centerInParent="true">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:id="@+id/pvIcon"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_above="@id/pvState"
                                    android:src="@drawable/ic_pv" />

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_above="@id/pvState"
                                    android:layout_toRightOf="@id/pvIcon">

                                    <RelativeLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true">

                                        <TextView
                                            android:id="@+id/tVprod"
                                            android:layout_width="90dp"
                                            android:layout_height="wrap_content"
                                            android:layout_above="@id/pvState"
                                            android:layout_centerVertical="true"
                                            android:text="N/A"
                                            android:textAlignment="center"
                                            android:textColor="@color/black"
                                            android:textSize="23dp"
                                            android:textStyle="bold" />

                                    </RelativeLayout>

                                </RelativeLayout>

                                <ProgressBar
                                    android:id="@+id/pvState"
                                    style="@android:style/Widget.ProgressBar.Horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"
                                    android:layout_marginBottom="10dp"
                                    android:indeterminate="false"
                                    android:paddingHorizontal="8dp"
                                    android:progressBackgroundTint="@color/light_blue_600"
                                    android:progressDrawable="@drawable/rectagle"
                                    android:progressTint="#d8cee1" />

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:paddingHorizontal="8dp">

                                    <TextView
                                        android:id="@+id/pvState2"
                                        android:layout_width="174px"
                                        android:layout_height="20dp"
                                        android:layout_alignParentBottom="true"
                                        android:layout_marginBottom="10dp"
                                        android:paddingHorizontal="8dp"
                                        android:background="@drawable/rectagle"
                                        android:backgroundTint="#8895ac"/>
                                </RelativeLayout>

                            </RelativeLayout>

                        </com.google.android.material.card.MaterialCardView>

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="184dp"
                        android:layout_height="121dp"
                        android:background="@color/gray_600">

                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="174dp"
                            android:layout_height="111dp"
                            android:layout_centerInParent="true">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:id="@+id/consIcon"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_above="@id/pvState"
                                    android:src="@drawable/ic_cons" />

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_above="@id/consState"
                                    android:layout_toRightOf="@id/consIcon">

                                    <RelativeLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true">

                                        <TextView
                                            android:id="@+id/tVcons"
                                            android:layout_width="90dp"
                                            android:layout_height="wrap_content"
                                            android:layout_above="@id/consState"
                                            android:layout_centerVertical="true"
                                            android:text="N/A"
                                            android:textAlignment="center"
                                            android:textColor="@color/black"
                                            android:textSize="23dp"
                                            android:textStyle="bold" />


                                    </RelativeLayout>

                                </RelativeLayout>

                                <ProgressBar
                                    android:id="@+id/consState"
                                    style="@android:style/Widget.ProgressBar.Horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"
                                    android:layout_marginBottom="10dp"
                                    android:indeterminate="false"
                                    android:paddingHorizontal="8dp"
                                    android:progressBackgroundTint="@color/light_blue_600"
                                    android:progressDrawable="@drawable/rectagle"
                                    android:progressTint="#d8cee1" />

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:paddingHorizontal="8dp">

                                    <TextView
                                        android:id="@+id/consState2"
                                        android:layout_width="174px"
                                        android:layout_height="20dp"
                                        android:layout_alignParentBottom="true"
                                        android:layout_marginBottom="10dp"
                                        android:background="@drawable/rectagle"
                                        android:backgroundTint="#8895ac"
                                        android:paddingHorizontal="8dp" />
                                </RelativeLayout>


                            </RelativeLayout>

                        </com.google.android.material.card.MaterialCardView>

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="184dp"
                        android:layout_height="121dp"
                        android:background="@color/gray_600">

                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="174dp"
                            android:layout_height="111dp"
                            android:layout_centerInParent="true">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:id="@+id/gridIcon"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_above="@id/gridState"
                                    android:src="@drawable/ic_grid" />

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_above="@id/gridState"
                                    android:layout_toRightOf="@id/gridIcon">

                                    <RelativeLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true">

                                        <TextView
                                            android:id="@+id/tVgrid"
                                            android:layout_width="90dp"
                                            android:layout_height="wrap_content"
                                            android:layout_above="@id/gridState"
                                            android:layout_centerVertical="true"
                                            android:text="N/A"
                                            android:textAlignment="center"
                                            android:textColor="@color/black"
                                            android:textSize="23dp"
                                            android:textStyle="bold" />

                                    </RelativeLayout>

                                </RelativeLayout>

                                <ProgressBar
                                    android:id="@+id/gridState"
                                    style="@android:style/Widget.ProgressBar.Horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"
                                    android:layout_marginBottom="10dp"
                                    android:indeterminate="false"
                                    android:paddingHorizontal="8dp"
                                    android:progressBackgroundTint="@color/light_blue_600"
                                    android:progressDrawable="@drawable/rectagle"
                                    android:progressTint="#d8cee1" />

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:paddingHorizontal="8dp"
                                    android:animateLayoutChanges="true">

                                    <TextView
                                        android:id="@+id/gridState2"
                                        android:layout_width="1px"
                                        android:layout_height="20dp"
                                        android:layout_alignParentBottom="true"
                                        android:layout_marginBottom="10dp"
                                        android:paddingHorizontal="8dp"
                                        android:background="@drawable/rectagle"
                                        android:backgroundTint="#BFB63434"
                                        android:layout_toRightOf="@id/gridmiddleleft"/>

                                    <ImageView
                                        android:id="@+id/gridmiddle"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:background="@drawable/ic_round_horizontal_rule_24"
                                        android:layout_centerInParent="true"
                                        android:layout_alignParentBottom="true"
                                        android:rotation="90" />

                                    <LinearLayout
                                        android:id="@+id/gridmiddle"
                                        android:layout_width="8px"
                                        android:layout_height="match_parent"
                                        android:layout_alignParentBottom="true"
                                        android:layout_centerInParent="true"
                                        android:layout_marginBottom="0dp" />

                                    <LinearLayout
                                        android:id="@+id/gridmiddleright"
                                        android:layout_width="8px"
                                        android:layout_height="match_parent"
                                        android:layout_alignParentBottom="true"
                                        android:layout_centerInParent="true"
                                        android:layout_marginBottom="0dp"
                                        android:layout_toRightOf="@id/gridmiddle"/>

                                    <LinearLayout
                                        android:id="@+id/gridmiddleleft"
                                        android:layout_width="8px"
                                        android:layout_height="match_parent"
                                        android:layout_alignParentBottom="true"
                                        android:layout_centerInParent="true"
                                        android:layout_marginBottom="0dp"
                                        android:layout_toLeftOf="@id/gridmiddle"/>

                                </RelativeLayout>

                            </RelativeLayout>

                        </com.google.android.material.card.MaterialCardView>

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="184dp"
                        android:layout_height="121dp"
                        android:background="@color/gray_600">

                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="174dp"
                            android:layout_height="111dp"
                            android:layout_centerInParent="true">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:id="@+id/battIcon"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_above="@id/pvState"
                                    android:src="@drawable/ic_battery_50" />

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_above="@id/battState"
                                    android:layout_toRightOf="@id/battIcon">

                                    <RelativeLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true">

                                        <TextView
                                            android:id="@+id/tVbatt"
                                            android:layout_width="90dp"
                                            android:layout_height="wrap_content"
                                            android:layout_above="@id/consState"
                                            android:layout_centerVertical="true"
                                            android:text="N/A"
                                            android:textAlignment="center"
                                            android:textColor="@color/black"
                                            android:textSize="23dp"
                                            android:textStyle="bold" />

                                    </RelativeLayout>

                                </RelativeLayout>

                                <ProgressBar
                                    android:id="@+id/battState"
                                    style="@android:style/Widget.ProgressBar.Horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"
                                    android:layout_marginBottom="10dp"
                                    android:indeterminate="false"
                                    android:paddingHorizontal="8dp"
                                    android:progressBackgroundTint="@color/light_blue_600"
                                    android:progressDrawable="@drawable/rectagle"
                                    android:progressTint="#d8cee1" />

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:paddingHorizontal="8dp"
                                    android:animateLayoutChanges="true">

                                    <TextView
                                        android:id="@+id/battState2"
                                        android:layout_width="1px"
                                        android:layout_height="20dp"
                                        android:layout_alignParentBottom="true"
                                        android:layout_marginBottom="10dp"
                                        android:layout_toLeftOf="@id/battmiddleright"
                                        android:background="@drawable/rectagle"
                                        android:backgroundTint="#A31BCA64"
                                        android:paddingHorizontal="8dp" />

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentBottom="true"
                                        android:layout_centerInParent="true"
                                        android:background="@drawable/ic_round_horizontal_rule_24"
                                        android:rotation="90" />

                                    <LinearLayout
                                        android:id="@+id/battmiddle"
                                        android:layout_width="8px"
                                        android:layout_height="match_parent"
                                        android:layout_alignParentBottom="true"
                                        android:layout_centerInParent="true"
                                        android:layout_marginBottom="0dp" />

                                    <LinearLayout
                                        android:id="@+id/battmiddleright"
                                        android:layout_width="8px"
                                        android:layout_height="match_parent"
                                        android:layout_alignParentBottom="true"
                                        android:layout_centerInParent="true"
                                        android:layout_marginBottom="0dp"
                                        android:layout_toRightOf="@id/battmiddle" />

                                    <LinearLayout
                                        android:id="@+id/battmiddleleft"
                                        android:layout_width="8px"
                                        android:layout_height="match_parent"
                                        android:layout_alignParentBottom="true"
                                        android:layout_centerInParent="true"
                                        android:layout_marginBottom="0dp"
                                        android:layout_toLeftOf="@id/battmiddle" />


                                </RelativeLayout>

                            </RelativeLayout>

                        </com.google.android.material.card.MaterialCardView>

                    </RelativeLayout>

                </GridLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="5dp"
                    android:paddingTop="5dp"
                    android:paddingRight="10dp"
                    android:paddingBottom="10dp"
                    android:layout_below="@id/gridlay">

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/webc"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <com.lit.fronuis.CustomGraph
                            android:id="@+id/graph"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"/>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="WIP"
                                android:textSize="30dp"
                                android:textStyle="bold"
                                android:layout_centerInParent="true"
                                />
                        </RelativeLayout>


                    </com.google.android.material.card.MaterialCardView>

                </RelativeLayout>

            </RelativeLayout>

        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:paddingRight="10dp">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true">

                <WebView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/webv"/>

            </com.google.android.material.card.MaterialCardView>

        </LinearLayout>

    </LinearLayout>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="start|top"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="@color/white"
        android:src="@drawable/ic_round_swap_horiz_24"
        tools:ignore="SpeakableTextPresentCheck" />
</RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>